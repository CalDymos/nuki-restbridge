{
  "name": "ESP32 POE ISO",
  "vendor": "Olimex",
  "url": "https://www.olimex.com/Products/IoT/ESP32/ESP32-POE-ISO/open-source-hardware",
  "frameworks": "arduino",
  "platform": "espressif32",
  "board": "esp32-poe-iso",

  "build": {
    "mcu": "esp32",
    "variant": "esp32",
    "f_cpu": "240000000L",
    "f_flash": "40000000L",
    "flash_mode": "dio",
    "core": "esp32",
    "arduino": {
      "ldscript": "esp32_out.ld"
    }
  },
  
    "connectivity": [
    "wifi",
    "bluetooth",
    "ethernet",
    "can"
  ],

  "upload": {
    "flash_size": "4MB",
    "maximum_ram_size": 327680,
    "maximum_size": 4194304,
    "speed": 921600
  },

  "debug": {
    "openocd_target": "esp32.cfg"
  }
}
